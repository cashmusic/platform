<!DOCTYPE html>

<!-- paulirish.com/2008/conditional-stylesheets-vs-css-hacks-answer-neither/ -->
<!--[if IE 8]>    <html class="no-js lt-ie9" lang="en"> <![endif]-->
<!--[if gt IE 8]><!--> <html class="no-js" lang="en"> <!--<![endif]-->
<head>
	<meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
	<link rel="icon" type="image/png" href="http://cashmusic.org/images/icons/cash.png" />

	<!-- Set the viewport width to device width for mobile -->
	<meta name="viewport" content="width=device-width" />

	<title>CASH Music / Admin</title>

	<link rel="icon" type="image/png" href="http://cashmusic.org/images/icons/cash.png" />
	<link href="{{www_path}}/ui/default/assets/css/app.css" rel="stylesheet" type="text/css" />
	<link href="{{www_path}}/ui/default/assets/css/offcanvas.css" rel="stylesheet" type="text/css" />
	<link href="{{www_path}}/ui/default/assets/css/responsive-tables.css" rel="stylesheet" type="text/css" />
	<link href="{{www_path}}/ui/default/assets/css/diy-ui/jquery-ui-1.8.23.custom.css" rel="stylesheet" type="text/css" />

	<script type="text/javascript">
		var cashAdminPath = "{{www_path}}";
		var currentSection = "{{section_name}}";
		document.write('<style type="text/css">#pagetips {display:none;}.nojs {display:none;}.onlyjs {display:inline;}</style>');
	</script>

	<script src="//www.google.com/jsapi"></script>
	<script src="{{www_path}}/ui/default/assets/scripts/modernizr.foundation.js" type="text/javascript"></script>
	<script src="{{jquery_url}}" type="text/javascript"></script>
	<script src="{{www_path}}/ui/default/assets/scripts/jquery-ui-1.9.0.custom.min.js" type="text/javascript"></script>
</head>

	<body id="page" class="off-canvas hide-extras">
	<div id="wrap">	
		<div id="chromespc" class="twelve columns phone-two hide-for-small">
			<div id="navmenumask">.</div>
			<div id="navmenu">
				<div id="assetstab" class="navitem bgcolor1{{#ui_current_assets}} currentnav{{/ui_current_assets}}">
					<h2><a href="{{www_path}}/assets/" class="navitemlink">{{link_menu_assets}}</a></h2>
				</div>
				<div id="peopletab" class="navitem bgcolor2{{#ui_current_people}} currentnav{{/ui_current_people}}">
					<h2><a href="{{www_path}}/people/" class="navitemlink">{{link_menu_people}}</a></h2>
				</div>
				<div id="commercetab" class="navitem bgcolor3{{#ui_current_commerce}} currentnav{{/ui_current_commerce}}">
					<h2><a href="{{www_path}}/commerce/" class="navitemlink">{{link_menu_commerce}}</a></h2>
				</div>
				<div id="calendartab" class="navitem bgcolor4{{#ui_current_calendar}} currentnav{{/ui_current_calendar}}">
					<h2><a href="{{www_path}}/calendar/" class="navitemlink">{{link_menu_calendar}}</a></h2>
				</div>
				<div id="elementstab" class="navitem bgcolor5{{#ui_current_elements}} currentnav{{/ui_current_elements}}">
					<h2><a href="{{www_path}}/elements/" class="navitemlink">{{link_menu_elements}}</a></h2>
				</div>
			</div>
		</div>

		<div id="mainspc" class="container {{specialcolor}}">
			<div id="smallchrome">&nbsp;</div>

			<div class="row">
				<section role="main">
					<div class="row">
						<div class="twelve columns phone-two">
							<p class="show-for-small">
								<a class="sidebar-button" id="sidebarButton" href="#sidebar"><span class="icon arrow-left menu-inactive"></span><span class="icon arrow-right menu-active"></span></a>
							</p>

							<div id="pagemenu">{{#section_menu}}{{{section_menu}}}{{/section_menu}}</div>

							<div id="subnavmenu" class="hide-for-small">
								<a href="{{www_path}}/"><span class="icon polaroid"></span> {{link_main_page}}</a>
								<div id="helpmenu">
									<span class="icon support"></span> {{link_menu_help}}
									<ul>
										<li><a href="#tips" id="tipslink">{{{misc_tips}}}</a></li>
										<li><a href="{{www_path}}/help/gettingstarted/">{{link_menu_help_gettingstarted}}</a></li>
										<li><a href="{{www_path}}/help/">{{link_help_alt}}</a></li>
										<li><a href="http://help.cashmusic.org/">help.cashmusic.org</a></li>
									</ul>
								</div>
								<div id="accountmenu">
									<span class="icon settings-2"></span> {{user_email}}
									<ul>
										<li><a href="{{www_path}}/account/">{{link_youraccount}}</a></li>
										<li><a href="{{www_path}}/settings/">{{link_settings}}</a></li>
										<li><a href="http://cashmusic.org/#more">{{action_donate}}</a></li>
										<li><a href="{{www_path}}/logout/" id="logout">{{action_logout}}</a></li>
									</ul>
								</div>
								<div class="clearfix">.</div>
							</div>

							<div id="tipspc" class="hide-for-small">
								<div id="pagetips">
									<span id="current_pagetip">{{{ui_page_tip}}}</span>
									<br />
									<a id="tipscloselink" href="#">{{action_close}}</a>
								</div>
							</div>
						</div>

						<div class="twelve columns">
							<h2 id="pagetitle">{{ui_title}}</h2>
							<div id="pagemessage">
								{{#error_message}}<p><span class="highlightcopy errormessage">{{error_message}}</span></p>{{/error_message}}
								{{#page_message}}<p><span class="highlightcopy">{{page_message}}</span></p>{{/page_message}}
							</div>
							<div id="pagedisplay">
								{{{content}}}
							</div>
						</div>
					</div>
				</section>

				<section id="sidebar" role="complementary">
					<nav id="sideMenu" role="navigation">
						<a href="{{www_path}}/assets/" class="button bgcolor1">{{link_menu_assets}}</a>
						<a href="{{www_path}}/people/" class="button bgcolor2">{{link_menu_people}}</a>
						<a href="{{www_path}}/commerce/" class="button bgcolor3">{{link_menu_commerce}}</a>
						<a href="{{www_path}}/calendar/" class="button bgcolor4">{{link_menu_calendar}}</a>
						<a href="{{www_path}}/elements/" class="button bgcolor5">{{link_menu_elements}}</a>
					</nav>

					<br />
					<div id="sideSubMenu" class="tar">
						<a href="{{www_path}}/"><b>{{link_main_page}}</b> <span class="icon polaroid"></span></a>
						<a href="{{www_path}}/account/">{{link_youraccount}} <span class="icon account"></span></a>
						<a href="{{www_path}}/settings/">{{link_settings}} <span class="icon settings-2"></span></a>
						<a href="{{www_path}}/help/">{{link_menu_help}} <span class="icon support"></span></a>
						<br />
						<a href="{{www_path}}/logout/">{{action_logout}} <span class="icon cancel-circle"></span></a>
					</div>
				</section>
			</div>
		</div>
	</div>

	<div id="footer">
		<div class="row">
			<div class="twelve columns">
				<p>{{{misc_footer_markup}}} <a href="{{www_path}}/licenses/" style="margin-left:0;">{{link_footer_learnmore}}</a>. {{#showterms}}<a href="{{www_path}}/terms/">{{link_footer_terms}}</a> {{/showterms}}{{#showprivacy}}<a href="{{www_path}}/privacy/">{{link_footer_privacy}}</a> {{/showprivacy}}<a href="http://cashmusic.org/#more" class="donatelink"><b>{{action_donate}}</b></a></p>
			</div>
		</div>
	</div>

	<!-- Included JS Files -->
	<script src="{{www_path}}/ui/default/assets/scripts/jquery.offcanvas.js" type="text/javascript"></script>
	<script src="{{www_path}}/ui/default/assets/scripts/responsive-tables.js" type="text/javascript"></script>
	<script src="{{www_path}}/ui/default/assets/scripts/jquery.iframe-transport.js" type="text/javascript"></script>
	<script src="{{www_path}}/ui/default/assets/scripts/jquery.fileupload.js" type="text/javascript"></script>
	<script src="{{www_path}}/ui/default/assets/scripts/jquery.admin.js" type="text/javascript"></script>
</body>
</html>