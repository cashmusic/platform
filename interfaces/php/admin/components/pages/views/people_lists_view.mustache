{{#ui_title}}
	<p>{{list_description}}</p>
	
	<h4>Actions</h4>
	<a href="{{www_path}}/people/lists/edit/{{list_id}}" class="mininav_flush"><i class="icon icon-pencil"></i>Edit</a>
	<a href="{{www_path}}/people/lists/export/{{list_id}}" class="mininav_flush noajax"><i class="icon icon-download-alt"></i>Export</a>
	<a href="{{www_path}}/people/lists/delete/{{list_id}}" class="mininav_flush needsconfirmation"><i class="icon icon-ban-circle"></i> Delete</a>
	<br /><br />

	<h4>Statistics</h4>
	<div class="col_oneoftwo">
	Total active members: {{analytics_active}}<br />
	Inactive members: {{analytics_inactive}}<br />
	</div><div class="col_oneoftwo lastcol">
	New members, past 7 days: {{analytics_last_week}}<br />
	</div><br />

	{{#list_members}}
		<br /><h4>List Members</h4>
		
		<table style="width:100%;">
			<colgroup style="width:32%;" />
			<colgroup style="width:23%;" />
			<colgroup style="width:12%;" />
			<colgroup />	

			<thead>
				<tr>
					<th scope="col">Email</th>
					<th scope="col">Name</th>
					<th scope="col">Joined</th>
					<th scope="col">Comment</th>
				</tr>
			</thead>

			<tbody>
			{{#list_members}}
			<tr>
				<td><a href="mailto:{{email_address}}">{{email_address}}</a></td>
				<td>{{display_name}}</td>
				<td>{{formatted_date}}</td>
				<td>{{initial_comment}}</td>
			</tr>
			{{/list_members}}
			</tbody>
		</table>
	{{/list_members}}
{{/ui_title}}
{{^ui_title}}
	<h4>Sorry</h4>Error getting list.
{{/ui_title}}