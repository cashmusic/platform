<div class="callout">
	<h4 class="fadedtext">Embed it</h4>
	<div class="drawer">
		<span class="descriptive fadedtext">Grab a code to embed your element in CASH page/embed templates or with HTML5, PHP, or WordPress.</span> <a class="drawerhandle spaced">codes</a>
		<div class="drawercontent">
			<br />
			{{#platform_type_single}}
				<b class="fadedtext codelabel">PHP: </b><code><div class="codearea" contenteditable="true">&lt;?php include_once('{{platform_path}}');CASHSystem::embedElement({{id}}); // ({{name}}) ?&gt;</div></code><br />
				<b class="fadedtext codelabel">WordPress: </b><code><div class="codearea" contenteditable="true">[cashmusic element=&quot;{{id}}&quot; name=&quot;{{name}}&quot;]</div></code>
			{{/platform_type_single}}
			<b class="fadedtext codelabel">Local: </b><code><div class="codearea" contenteditable="true">&#123;&#123;&#123;element_{{id}}&#125;&#125;&#125;</div></code><br />
			<b class="fadedtext codelabel">HTML5: </b><code><div class="codearea" contenteditable="true">&lt;script type=&quot;text/javascript&quot; src=&quot;{{public_url}}cashmusic.js&quot;&gt;&lt;/script&gt;&lt;script type=&quot;text/javascript&quot;&gt; window.cashmusic.embed('{{public_url}}','{{id}}');&lt;/script&gt;</div></code>
		</div>
	</div>
</div>



<div class="drawer noprefix element_embedtemplate">
	<div class="fadedtext"><b>HTML5 embed template:</b> {{embed_template_name}} <span class="spaced">/</span> <span class="spaced">
		{{#defined_embed_templates}}<a class="drawerhandle fullsize"><i class="icon icon-columns"></i><b>Pick another template</b></a></span></div>{{/defined_embed_templates}}
		{{^defined_embed_templates}}<a href="{{www_path}}/elements/templates/add/"><i class="icon icon-list-alt"></i><b>Add a new embed template</b></a></span></div>{{/defined_embed_templates}}
	<div class="drawercontent col_oneoftwo">
		<br />
		<form method="post" action="">
			<label for="template_id">Choose an embed template:</label>
			<div class="row">
				<div class="six columns">
					<select id="change_template_id" name="change_template_id">
						{{{template_options}}}
					</select>
				</div>
				<div class="six columns">
					<input type="submit" value="use this template" />
				</div>
			</div>
		</form>
		<br />
	</div>
</div>

<br /><br />

<h4>Statistics</h4> <span class="descriptive">Total unique views: {{total_views}}</span><br />

{{#location_analytics}}
	<div class="drawer">
		<div class="drawercontent">
			<br />
			{{#location_analytics}}
				<h5>Views by location</h5>
				<table style="width:100%;" class="responsive">
					<colgroup style="width:85%;" /><colgroup />	
					<thead>
						<tr>
							<th scope="col">Element location</th>
							<th scope="col">Views</th>
						</tr>
					</thead>
					<tbody>
					{{#location_analytics}}
					<tr>
						<td>{{access_location}}</td>
						<td>{{total}}</td>
					</tr>
					{{/location_analytics}}
					</tbody>
				</table>
			{{/location_analytics}}

			{{#method_analytics}}
				<br />
				<h5>Views by request method</h5>
				<table style="width:100%;" class="responsive">
					<colgroup style="width:85%;" /><colgroup />	
					<thead>
						<tr>
							<th scope="col">Request method</th>
							<th scope="col">Views</th>
						</tr>
					</thead>
					<tbody>
					{{#method_analytics}}
					<tr>
						<td>{{access_method}}</td>
						<td>{{total}}</td>
					</tr>
					{{/method_analytics}}
					</tbody>
				</table>
			{{/method_analytics}}
			<br />
		</div>
		<a class="drawerhandle mininav_flush usecolor5">detailed statistics</a>
	</div>
{{/location_analytics}}

<br /><br />
